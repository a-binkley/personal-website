{
	"name": "personal-website",
	"version": "1.0.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.5.2",
		"@types/node": "^16.18.57",
		"@types/react": "^18.2.25",
		"@types/react-dom": "^18.2.10",
		"bootstrap-icons": "^1.11.1",
		"buffer": "^6.0.3",
		"csstype": "^3.1.2",
		"moment": "^2.29.4",
		"nth-check": "^2.0.1",
		"octokit": "^3.1.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-scripts": "^5.0.1",
		"react-test-renderer": "^18.2.0",
		"typescript": "^4.9.5",
		"web-vitals": "^2.1.4",
		"ws": "^8.17.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --coverage --watchAll=false",
		"eject": "react-scripts eject",
		"lint": "npx eslint ."
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"jest": {
		"collectCoverageFrom": [
			"./src/NetworkRequests.ts",
			"./src/components/**",
			"!./src/components/*/index.ts"
		],
		"coverageThreshold": {
			"global": {
				"statements": 90,
				"branches": 90,
				"functions": 90,
				"lines": 90
			}
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/react-test-renderer": "^18.0.5",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"eslint": "^8.51.0",
		"react-router-dom": "^6.16.0"
	}
}
